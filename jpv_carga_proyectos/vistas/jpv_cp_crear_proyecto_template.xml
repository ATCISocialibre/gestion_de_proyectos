<?xml version="1.0"?>
    <openerp>
        <data>
            <template id="ept_carga_proyectos.registrar_proyecto_template" name="Template para el registro de proyecto">
                <div class="panel panel-default hidden">
                    <div class="panel-heading">Entidad Político Territorial</div>
                    <div class="panel-body">
                        <label for="usr">Entidad</label>
                            <select class="selectpicker show-tick" id="entidad" name="partner_id">
                                <t t-foreach="partner_data" t-as="partner">
                                    <option t-att-value="'%s' % (partner.id)" ><t t-esc="partner.name"/></option>
                                </t>
                            </select>
                        <label for="usr">Cuenta</label>
                            <select class="selectpicker show-tick" name="cuenta_id" id="cuenta">
                                <t t-foreach="cuenta_data" t-as="cuenta">
                                    <option t-att-value="'%s' % (cuenta.id)"><t t-esc="cuenta.name"/></option>
                                </t>
                            </select>
                    </div>
            </div>    
            <div class="panel panel-default">
                <div class="panel-heading">Nombre y Descripción<a href="#" id="ayudas_informacion" ref="nombre_descripcion"><img class="pull-right" src="/ept_carga_proyectos/static/src/img/dialog_information.png"/></a></div>
                <div class="panel-body">
                    <label for="nombre_proyecto" data-toggle="popover" data-trigger="hover" data-content="Nombre del proyecto a cargar">Nombre del Proyecto</label>
                        <textarea class="form-control" name="nombre_proyecto" mensaje="Nombre del Proyecto" required="required"  rows="3" maxlength="500"  ></textarea>
                        <br/>
                    <label for="descripcion_proyecto" data-toggle="popover" data-trigger="hover" data-content="Descripción del proyecto a cargar">Descripción del Proyecto</label>
                        <textarea  class="form-control" name="descripcion_proyecto" mensaje="Descripción del Proyecto" required="required"  rows="4" maxlength="500"></textarea>
                </div>
            </div>
            <div class="row">
                <div class="col-md-12">
                    <div class="panel panel-default">
                        <div class="panel-heading">Tiempo de Ejecución Estimado<a href="#" id="ayudas_informacion" ref="tiempo_ejecucion"><img class="pull-right" src="/ept_carga_proyectos/static/src/img/dialog_information.png"/></a></div>
                        <div class="panel-body">
                            <div class="col-md-4">
                                <label for="fecha_inicio" data-toggle="popover" data-trigger="hover" data-content="Fecha de inicio estimada del proyecto a cargar">Fecha de Inicio</label>
                                <input type="text" class="form-control border_form_input date_proyecto_ept"  id="fecha_inicio" name="fecha_inicio" mensaje="Fecha de inicio" required="required"/>
                            </div>
                            <div class="col-md-4">
                                    <label for="fecha_fin" data-toggle="popover" data-trigger="hover" data-content="Fecha de fin estimada del proyecto a cargar">Fecha de Fin</label>
                                    <input type="text" class="form-control border_form_input date_proyecto_ept"  id="fecha_fin" name="fecha_fin" mensaje="Fecha de Fin" required="required"/>
                            </div>
                            <div class="col-md-4">
                                    <label for="duracion_proyec" data-toggle="popover" data-trigger="hover" data-content="Duración estimada del proyecto">Duración del proyecto</label>
                                    <p id="duracion_proyecto"></p>
                                    <input type="text" class="form-control control-label hidden" disabled="" id="duracion_proyec" name="duracion_proyec"/>
                            </div>
                        </div>
                    </div>       
                </div>
                <div class="col-md-12">
                    <div class="panel panel-default">
                        <div class="panel-heading">Monto del Proyecto<a href="#" id="ayudas_informacion" ref="monto_proyecto"><img class="pull-right" src="/ept_carga_proyectos/static/src/img/dialog_information.png"/></a></div>
                        <div class="panel-body">
                            <div class="col-md-6">
                                <label for="monto_proyecto" data-toggle="popover" data-trigger="hover" data-content="Monto disponilble del proyecto">Monto del Proyecto</label>
                                <input type="text" class="form-control monto" name="monto_proyecto" id="monto_proyecto" mensaje="Monto Proyecto" required="required"></input>
                            </div>
                            <div class="col-md-6">
                                <label for="monto_disponible" data-toggle="popover" data-trigger="hover" data-content="Monto disponible del la entidad">Monto Disponible</label>
                                <p id="monto_disponible" class="monto"><t t-esc="monto_total"/></p>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-md-12">
                    <div class="panel panel-default">
                        <div class="panel-heading">Clasificación de Inversión<a href="#" id="ayudas_informacion" ref="clasificacion"><img class="pull-right" src="/ept_carga_proyectos/static/src/img/dialog_information.png"/></a></div>
                        <div class="panel-body">             
                            <div class="col-md-12">
                                <label for="tipo_sector" data-toggle="popover" data-trigger="hover" data-content="Tipo de sector del proyecto a cargar">Tipo de Sector</label>
                                    <select class="form-control" id="tipo_sector" name="tipo_sector_id" mensaje="Tipo de sector" required="required">
                                        <option class="hidden" selected="true" value=''>Seleccione</option>
                                        <t t-foreach="tipo_sectores_data" t-as="sector">
                                            <option t-att-value="'%s' % (sector.id)"><t t-esc="sector.name"/></option>
                                        </t>
                                    </select>
                                <label for="categoria"  data-toggle="popover" data-trigger="hover" data-content="Categoría del proyecto a cargar">Categoria</label>
                                    <select class="form-control" id="categoria" name="categoria_id" mensaje="Categoría" required="required">
                                        <option class="hidden" value=''>Seleccione</option>
                                    </select>
                                <label for="subcategoria" data-toggle="popover" data-trigger="hover" data-content="Subcategoía del proyecto a cargar">Subcategoria</label>
                                    <select class="form-control" id="subcategoria" name="subcategoria_id" mensaje="Subcategoria" required="required">
                                        <option class="hidden"  value=''>Seleccione</option>
                                    </select>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="panel panel-default">
                <div class="panel-heading">Proyecto de Mantenimiento<a href="#" id="ayudas_informacion" ref="proyecto_mantenimiento"><img class="pull-right" src="/ept_carga_proyectos/static/src/img/dialog_information.png"/></a></div>
                    <div class="panel-body">
                        <div class="col-md-12">
                            <div class="checkbox col-md-6">
                                <label>
                                    <input type="checkbox" value="" id="proyect_mantenimiento" name="proyect_mantenimiento"  data-switch-no-init="True" data-toggle="popover" data-trigger="hover" data-content="Seleccione si este proyecto es de mantenimiento">
                                        ¿Este proyecto es de mantenimiento?
                                    </input>
                                </label>
                            </div>
                            <div class="col-md-6 hidden" id="monto_mant_disp">
                                <label data-toggle="popover" data-trigger="hover" data-content="Monto disponible del la entidad">Monto Disponible de Mantenimiento</label>
                                    <p class="monto" id="monto_disponible_mantenimiento"><t t-esc="monto_manteni_disp"/></p>

                            </div>
                        </div>
                    </div>
            </div>
            <div class="row" id="caracteristicas_generales">
