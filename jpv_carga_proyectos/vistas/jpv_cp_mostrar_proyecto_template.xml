<?xml version="1.0"?>
    <openerp>
        <data>
            <template id="jpv_carga_proyectos.modal_cancelar_proyectos" name="template para cancelar proyectos">
                <div class="modal fade" id="modal-switch" role="dialog" aria-labelledby="AJAXErrorModal" aria-hidden="true" >
                    <div class="modal-dialog">
                        <div class="modal-content ">
                            <div class="modal-header alert alert-info">
                                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&amp;times;</button>
                                <h4 class="modal-title titulo " >Cancelar Proyecto</h4>
                            </div>
                            <div class="modal-body cuerpo alert alert-danger" ><p>¿Esta seguro que desea cancelar el proyecto?.</p> 
                            <br/>
                            <form   action="/proyecto/cancelar">
                                <input type="text" class="form-control hidden" name="proyecto_id" id="proyecto_id" t-att-value="'%s' % (proyecto.id)"/>
                                <button
                                    type="button"
                                    class="btn btn-danger" id="cancelar_proyecto_confirm" >
                                    Acepto Cancelar el proyecto <t t-esc="proyecto['correlativo']"/>
                                </button>
                            </form>
                            </div>
                            <div class="modal-footer "><span class="pie"></span><button type="button" class="btn btn-primary" data-dismiss="modal">Cerrar</button></div>
                        </div>
                    </div>
                </div>
            </template>
            
            
            
            
            
            <template id="jpv_carga_proyectos.proyecto_vista_solo_lectura" name="Template para mostrar el proyecto solo lectura">
                <div> 
                <t t-foreach="carga_proyecto_data" t-as="proyectos">
                    <a href="/proyecto/crear">
                        <button type="button"  t-attf-class="btn btn-info glyphicon glyphicon-folder-open" > Crear</button>
                    </a>
                    <t t-if="proyectos['state'] in ['aprobado','carga','diferido']">
                        <a t-att-href="'/proyecto/editar/%s' % (carga_proyecto_data['id'])">
                            <button type="button"  t-attf-class="btn btn-info glyphicon glyphicon-folder-open" > Editar</button>
                        </a>    
                    </t>
                    <t t-if="proyectos['state'] in ['carga','diferido']">
                        <a>
                            <button type="button" id="cancelar_proyecto" data-toggle="modal" data-target="#modal-switch" t-attf-class="btn btn-danger glyphicon glyphicon-folder-open" > Cancelar</button>
                        </a>
                    </t>
                    <div class="pull-right">
                        <t t-if="proyectos['state']=='aprobado'">
                                <img  src="/jpv_carga_proyectos/static/src/img/status.png"><span class="glyphicon glyphicon-check" /> APROBADO</img>
                        </t>
                        <t t-if="proyectos['state']=='negado'">
                                <img  src="/jpv_carga_proyectos/static/src/img/status_busy.png"><span class="glyphicon glyphicon-remove-circle"  /> NEGADO</img>
                        </t>
                        <t t-if="proyectos['state']=='evaluacion'">
                               <img  src="/jpv_carga_proyectos/static/src/img/bullet_blue.png"><span class="glyphicon glyphicon-pencil" /> EN EVALUACIÓN</img>
                        </t>
                        <t t-if="proyectos['state']=='cancelado'">
                                <img  src="/jpv_carga_proyectos/static/src/img/status_busy.png"><span class="glyphicon glyphicon-hand-down" /> CANCELADO</img>
                        </t>
                        <t t-if="proyectos['state']=='carga'">
                                <img  src="/jpv_carga_proyectos/static/src/img/status_offline.png"><span class="glyphicon glyphicon-list-alt" /> BORRADOR</img>
                        </t>
                        <t t-if="proyectos['state']=='diferido'">
                               <img  src="/jpv_carga_proyectos/static/src/img/status_away.png"><span class="glyphicon glyphicon-refresh" /> DIFERIDO</img>
                        </t>
                        <t t-if="proyectos['state']=='culminado'">
                               <img  src="/jpv_carga_proyectos/static/src/img/status.png"><span class="glyphicon glyphicon-flag" /> CULMINADO</img>
                        </t>
                    </div>
                </t>
                </div>
                <br/><br/>
                <t t-foreach="carga_proyecto_data" t-as="proyecto">
