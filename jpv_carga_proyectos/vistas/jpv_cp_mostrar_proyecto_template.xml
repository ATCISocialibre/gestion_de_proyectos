                <div class="panel panel-default">
                    <div class="panel-heading">Beneficiarios del Proyecto</div>
                    <div class="panel-body">
                        <table class="table table-bordered table-striped mt32">
                            <thead>
                                <tr>
                                    <th>Masculino</th>
                                    <th>Femenino</th>
                                    <th>Total</th>
                                </tr>
                            </thead>
                            <tbody>
                                <tr class="active" >
                                    <td ><t t-esc="proyecto.benef_masculino"/></td>
                                    <td ><t t-esc="proyecto.benef_femenino"/></td>
                                    <td ><t t-esc="proyecto.total_benef"/></td>
                                </tr>
                            </tbody>
                        </table> 
                    </div>
                </div>
                <div class="panel panel-default">
                    <div class="panel-heading">Empleo Asociado</div>
                    <div class="panel-body">
                        <table class="table table-bordered table-striped mt32">
                            <thead>
                                <tr>
                                    <th>Tipo</th>
                                    <th>Masculino</th>
                                    <th>Femenino</th>
                                    <th>Total</th>
                                </tr>
                            </thead>
                            <tbody>
                                <tr class="active" >
                                    <td><h4>Directos</h4></td>
                                    <td ><t t-esc="proyecto.empleo_direct_masculino"/></td>
                                    <td ><t t-esc="proyecto.empleo_direct_femenino"/></td>
                                    <td ><t t-esc="proyecto.empleo_direct_total"/></td>
                                </tr>
                                <tr class="active" >
                                    <td><h4>Indirectos</h4></td>
                                    <td ><t t-esc="proyecto.empleo_indirect_masculino"/></td>
                                    <td ><t t-esc="proyecto.empleo_indirect_femenino"/></td>
                                    <td ><t t-esc="proyecto.empleo_indirect_total"/></td>
                                </tr>
                            </tbody>
                        </table> 
                    </div>
                </div>
                <div class="col-md-12">
                    <div class="panel panel-default">
                        <div class="panel-heading">Política Territorial</div>
                            <div class="panel-body">
                                <label for="estado_id">Estado</label>
                                <p name="estado_id"><t t-esc="proyecto.estado_id.estado"/></p>
                                <label for="municipio_id">Municipio</label>
                                <p name="municipio_id"><t t-esc="proyecto.municipio_id.municipio"/></p>
                                <label for="parroquia">Parroquia</label>
                                <p name="parroquia_id"><t t-esc="proyecto.parroquia_id.parroquia"/></p>
                                <label for="sector">Sector</label>
                                <p name="sector"><t t-esc="proyecto.sector"/></p>
                            </div>
                        </div>       
                    </div>
                    <div class="col-md-12">
                        <div class="panel panel-default">
                            <div class="panel-heading">Geo-espacial</div>
                                <div class="panel-body">
                                    <label for="huso">Huso Inicio</label>
                                    <p name="huso"><t t-esc="proyecto.huso_id.huso"/></p>
                                    <label for="coord_e_i">Coordenas Este Inicio</label>
                                    <p name="coord_este"><t t-esc="proyecto.coord_este"/></p>
                                    <label for="coord_n_i">Coordenas Norte Inicio</label>
                                    <p name="coord_norte"><t t-esc="proyecto.coord_norte"/></p>
                                    <t t-if="proyecto.coordenada==True">
                                        <label for="huso">Huso Final</label>
                                        <p name="huso"><t t-esc="proyecto.husof_id.huso"/></p>
                                        <label for="coord_e_i">Coordenas Este Fin</label>
                                        <p name="coord_este_f"><t t-esc="proyecto.coord_este_f"/></p>
                                        <label for="coord_n_i">Coordenas Norte Fin</label>
                                        <p name="coord_norte_f"><t t-esc="proyecto.coord_norte_f"/></p>
                                    </t>
                                </div>
                            </div>
                        </div>
                    <div class="row">
                        <t t-if="len(proyecto.foto_id)">
                            <input name="objeto_fotos_obj" type="hidden" t-att-value="'%s' % (proyecto.foto_id[0])"/>
                            <input name="objeto_fotos_datas" type="hidden" t-att-value="'%s' % (proyecto.foto_id[0].datas)"/>
                            <input name="objeto_fotos_name" type="hidden" t-att-value="'%s' % (proyecto.foto_id[0].name)"/>
                            <input name="objeto_fotos_type" type="hidden" t-att-value="'%s' % (proyecto.foto_id[0].mimetype)"/>
                            <input name="objeto_fotos_id" type="hidden" t-att-value="'%s' % (proyecto.foto_id[0].id)"/>
                        </t>
                        <input name="solo_lectura" type="hidden" id="solo_lectura" value="True"/>
                        <div class="col-md-12">
                            <div class="panel panel-default">
                                <div class="panel-heading">Fotografia<a href="#" id="ayudas_informacion" ref="fotografia"><img class="pull-right" src="/jpv_carga_proyectos/static/src/img/dialog_information.png"/></a></div>
                                <div class="panel-body">
                                    <div class="col-md-12">
                                        <div class="col-md-9">
                                            <input type="file" class="file-loadig input-imagen_proyecto" name="fotos_id" data-toggle="popover" data-trigger="hover" data-content="Fotografia inicial del proyecto a cargar"></input>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                    <t t-call="jpv_carga_proyectos.modal_cancelar_proyectos"></t>
            </t>
</template>

</data>
</openerp>

