<?xml version="1.0"?>
<openerp>
<data>
    

    <record model="ir.ui.view" id="view_ept_ent_tipo_entidades_form">
        <field name="name">ept_ent.tipo_entidades.form</field>
        <field name="model">ept_ent.tipo_entidades</field>
        <field name="type">form</field>
        <field name="arch" type="xml">
            <form string="Tipos de Entidad">
                <sheet>
                    <group>
                        <separator string="Registro del Tipo de Entidades" colspan="4"/>
                        <field name="name" style="width:50%"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
        
    <record model="ir.ui.view" id="view_ept_ent_tipo_entidades_tree">
        <field name="name">ept_ent.tipo_entidades.tree</field>
        <field name="model">ept_ent.tipo_entidades</field>
        <field name="type">tree</field>
        <field name="arch" type="xml">
            <tree string="Tipos de Entidad">
                <field name="name"/>
            </tree>
        </field>
    </record>
    
    <record model="ir.actions.act_window" id="action_ept_ent_tipo_entidades">
        <field name="name">Registro de Tipos Entidad</field>
        <field name="res_model">ept_ent.tipo_entidades</field>
        <field name="view_type">form</field>
        <field name="help" type="html">
            <p class="oe_view_nocontent_create">
            Aqui se registran los Tipos de Entidad
            </p>
        </field>
    </record>

        <menuitem name="Tipos de Entidad" 
            id="menu_ept_ent_tipo_entidades" 
            parent="ept_usuarios.menu_general_ept_pi" 
            sequence="10"
            groups="base.group_system"
            />
            <menuitem name="Registro de Tipos de Entidad" 
                id="regsitro_ept_ent_tipo_entidades" 
                action="action_ept_ent_tipo_entidades" 
                parent="menu_ept_ent_tipo_entidades" 
                sequence="3"
                />

        
</data>
</openerp>
