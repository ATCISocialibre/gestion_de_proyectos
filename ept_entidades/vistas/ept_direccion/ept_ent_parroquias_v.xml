<?xml version="1.0" encoding="utf-8"?>
   <openerp>
     <data>
    <!--REGISTRO DE PARROQUIAS-->
        
    <record model="ir.ui.view" id="view_ept_ent_parroquias_form">
        <field name="name">ept_ent.parroquias.form</field>
        <field name="model">ept_ent.parroquias</field>
        <field name="type">form</field>
        <field name="arch" type="xml">
            <form string="Dirección de Parroquias">
                <sheet>
                <group>
                <field name="parroquia" select="1"/>
                <field name="codigo" select="2"/>
                <field name="municipio_id" select="0" attrs="{'readonly': [('municipio_id','>',1)]}"/>
                <field name="active" select="0"/>
                </group>
                </sheet>
            </form>
        </field>
    </record>
    <record model="ir.ui.view" id="view_ept_ent_parroquias_tree">
        <field name="name">ept_ent.parroquias.tree</field>
        <field name="model">ept_ent.parroquias</field>
        <field name="type">tree</field>
        <field name="arch" type="xml">
            <tree string="Dirección de Parroquias">
                <field name="parroquia"/>
                <field name="municipio_id"/>
                <field name="codigo"/>
                <field name="active"/>
            </tree>
        </field>
    </record>
    <record model="ir.actions.act_window" id="action_ept_ent_parroquias">
        <field name="name">Parroquias</field>
        <field name="res_model">ept_ent.parroquias</field>
        <field name="view_type">form</field>
        <field name="view_mode">tree,form</field>
    </record>

    
    <menuitem name="Parroquias" id="menu_res_parroquias" parent="ept_entidades.next_id_82" action="action_ept_ent_parroquias" sequence="5"/>

    </data>
</openerp>
