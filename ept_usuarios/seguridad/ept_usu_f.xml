<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
<record id="mi_ure_entidad_usuarios" model="ir.rule">
            <field name="name">Usuarios de las entidades que pueden ver los  coordinadores de las ures</field>
            <field name="model_id" ref="base.model_res_users"/>
            <field name="domain_force">['|',('id','in',[ u.id for ur in user.ures_ids for u in ur.user_ids]),('id','in',[ u.id for ur in user.ures_ids for e in ur.entidad_ids for u in e.user_ids])]</field>
            <field name="perm_read" eval='True'/>
            <field name="perm_write" eval='True'/>
            <field name="perm_create" eval='True'/>
            <field name="perm_unlink" eval='True'/>
            <field name="groups" eval="[(4,ref('ept_usuarios.group_coor_ures'))]"/>
        </record>
 </data>
</openerp>
