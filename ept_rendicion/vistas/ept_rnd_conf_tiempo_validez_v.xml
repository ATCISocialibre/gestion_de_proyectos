<?xml version="1.0" encoding="utf-8"?>
   <openerp>
     <data>
         

    <record model="ir.ui.view" id="view_ept_rnd_tiempo_validez_form">
        <field name="name">ept_rnd_tiempo_validez</field>
        <field name="model">ept_rnd.tiempo_validez</field>
        <field name="type">form</field>
        <field name="arch" type="xml">
            <form string="Tiempo de Validez" >
                <sheet>
                    <separator string="Tiempo de Validez" colspan="4"/>
                    <group>
                        <field  name="dias_validez" />
                        <field  name="model_id" />
                        <field  name="active" />
                        </group>
                </sheet>
            </form>
        </field>
    </record>

     
     <record model="ir.ui.view" id="view_ept_rnd_tiempo_validez_tree">
        <field name="name">ept_rnd_tiempo_validez</field>
        <field name="model">ept_rnd.tiempo_validez</field>
        <field name="type">tree</field>
        <field name="arch" type="xml">
            <tree string="Tiempo de validez">           
                <field  name="dias_validez" />
                <field  name="model_id" />
                <field  name="active" />
            </tree>
        </field>
     </record>
     

    
    <record model="ir.actions.act_window" id="action_ept_rnd_tiempo_validez">
        <field name="name">Avances</field>
        <field name="res_model">ept_rnd.tiempo_validez</field>
        <field name="view_type">form</field>
        <field name="view_mode">tree,form</field>
        <field name="view_id" ref="view_ept_rnd_tiempo_validez_tree"/>
        <field name="help" type="html">
            <p class="oe_view_nocontent_create">
            Aqui se registran los items de los avances
            </p>
        </field>
    </record>

            
        <menuitem name="Tiempo de Validez" 
            id="menu_ept_rnd_tiempo_validez"  
            action="action_ept_rnd_tiempo_validez" 
            parent="menu_ept_rendiciones_avances"
            sequence="20"/>
            
    </data>
</openerp>
