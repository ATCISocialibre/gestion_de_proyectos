<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>
    <template id="jpv_index.proyecto" name="proyectos"> 
      <t t-call="jpv_index.lib_proyecto" />
          <div class="container">
          <!-- El menú de navegación justificada es para una sola línea por cada elemento de la lista.
               Líneas múltiples requerirán código personalizado mas elaborado. -->
          <!--Menu-->
          <div class="masthead">
            <img src="/jpv_index/static/src/imagenes/logo1.png"/> 
            <nav>
              <ul class="nav nav-justified">
                <li><a href="/">Inicio</a></li>
                <li class="active"><a href="/paginas/proyecto">Proyectos</a></li>
                <li><a href="/paginas/financiamiento">Financiamiento</a></li>
                <li><a href="/paginas/estadistica">Estadisticas</a></li>
              </ul>
            </nav>
          </div>
           <!--Menu-->

          <!-- Jumbotron -->
          <div class="jumbotron">
            <h1>Proyectos 2014-2015</h1>
            <p class="lead">Principales Proyectos finaciados en la gestión 2014-2015.</p>
          </div>
          <div class="row">
            <div class=" col-md-12">
            <section class="content">
              <div class="row">
                <div class="col-md-6">
                  <!-- AREA CHART -->
                  <div class="box box-primary">
                    <div class="box-header with-border">
                      <h3 class="box-title">Reporte Semestral</h3>
                      <div class="box-tools pull-right">
                      </div>
                    </div>
                    <div class="box-body">
                      <div class="chart">
                        <canvas id="areaChart" style="height:250px"></canvas>
                      </div>
                    </div><!-- /.box-body -->
                  </div><!-- /.box -->

                  <!-- DONUT CHART -->
                  <div class="box box-danger">
                    <div class="box-header with-border">
                      <h3 class="box-title">Principales Proyectos</h3>
                      <div class="box-tools pull-right">
                      </div>
                    </div>
                    <div class="box-body">
                        <canvas id="pieChart" style="height:250px"></canvas>
                    </div><!-- /.box-body -->
                  </div><!-- /.box -->

                </div><!-- /.col (LEFT) -->
                <div class="col-md-6">
                  <!-- LINE CHART -->
                  <div class="box box-info">
                    <div class="box-header with-border">
                      <h3 class="box-title">Reporte Semestral</h3>
                      <div class="box-tools pull-right">
                      </div>
                    </div>
                    <div class="box-body">
                      <div class="chart">
                        <canvas id="lineChart" style="height:250px"></canvas>
                      </div>
                    </div><!-- /.box-body -->
                  </div><!-- /.box -->

                  <!-- BAR CHART -->
                  <div class="box box-success">
                    <div class="box-header with-border">
                      <h3 class="box-title">Indice de Proyectos Semestral</h3>
                      <div class="box-tools pull-right">
                      </div>
                    </div>
                    <div class="box-body">
                      <div class="chart">
                        <canvas id="barChart" style="height:230px"></canvas>
                      </div>
                    </div><!-- /.box-body -->
                  </div><!-- /.box -->

                </div><!-- /.col (RIGHT) -->
              </div><!-- /.row -->
            </section><!-- /.content -->
            </div>        
          </div>
        </div> <!-- /container -->
       <footer>
        <div class="footer">
